{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest",
    "format": "prettier --write 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "lint": "eslint '{src,test}/**/*.{ts,tsx}'",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "start:prod": "pm2 start ecosystem.config.js",
    "stop": "pm2 delete all",
    "submit-scores": "tsx scripts/submit-scores-loop.ts",
    "seed:leaderboard": "tsx scripts/seed-leaderboard.ts",
    "check:leaderboard": "tsx scripts/check-leaderboard.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d ./src/infra/db/dataSource.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/infra/db/dataSource.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/infra/db/dataSource.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@noble/hashes": "^2.0.1",
    "@noble/secp256k1": "^2.3.0",
    "@stacks/network": "^7.2.0",
    "@stacks/transactions": "^7.3.0",
    "@stacks/wallet-sdk": "^7.2.0",
    "bip32": "^5.0.0",
    "bullmq": "^5.66.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "fastify-raw-body": "^5.0.0",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "secp256k1": "link:@@noble/secp256k1",
    "tiny-secp256k1": "^2.2.4",
    "typeorm": "^0.3.28",
    "zod": "^4.1.13"
  }
